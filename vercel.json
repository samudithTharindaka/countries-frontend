{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },{
      "src": "/home",
      "dest": "/"
    },
    {
      "src": "/main",
      "dest": "/"
    }
    ,
    {
      "src": "/countries",
      "dest": "/"
    }
  
  ]
}